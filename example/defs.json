{
  "volumes": [
    {
      "Name": "example",
      "Labels": {
        "co.imti.iotagent.example": "example-volume"
      },
      "Driver": "local"
    }
  ],
  "networks": {
    "example-net": {
      "Driver": "bridge",
      "CheckDuplicate": true,
      "EnableIPv6": false,
      "IPAM": {
        "Driver": "default",
        "Config": [
          {
            "Subnet": "172.27.0.0/16",
            "IPRange": "172.27.10.0/24",
            "Gateway": "172.27.10.11"
          }
        ]
      },
      "Labels": {
        "co.imti.iotagent.example": "example-network"
      }
    }
  },
  "containers": [
    {
      "Name": "example-a",
      "Image": "alpine",
      "NetworkingConfig": {
        "EndpointsConfig": {
          "byp-net": {}
        }
      },
      "Cmd": [
        "ping",
        "8.8.8.8"
      ]
    }
  ]
}