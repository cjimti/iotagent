{
  "volumes": [
    {
      "Name": "example",
      "Labels": {
        "co.imti.iotagent.example": "example-volume"
      },
      "Driver": "local"
    }
  ],
  "networks": {
    "example-net": {
      "Driver": "bridge",
      "CheckDuplicate": true,
      "EnableIPv6": false,
      "IPAM": {
        "Driver": "default",
        "Config": [
          {
            "Subnet": "172.27.0.0/16",
            "IPRange": "172.27.10.0/24",
            "Gateway": "172.27.10.11"
          }
        ]
      },
      "Labels": {
        "co.imti.iotagent.example": "example-network"
      }
    }
  },
  "containers": {
    "example-a": {
      "Config": {
        "Image": "alpine",
        "Cmd": [
          "ping",
          "8.8.8.8"
        ]
      },
      "NetworkingConfig": {
        "EndpointsConfig": {
          "example-net": {}
        }
      },
      "HostConfig": {
        "PortBindings": {
          "2701/tcp": [{ "HostPort": "2701" }]
        },
        "Privileged": true,
        "RestartPolicy": {"Name": "unless-stopped"},
        "LogConfig": {
          "Type": "json-file",
          "Config": {
            "max-buffer-size": "1m",
            "max-size": "5m",
            "mode": "non-blocking"
          }
        }
      }
    }
  }
}